/****************** Fully Implicit ******************/
/*!
 * \defgroup BoxModel
 * \brief Fully Implicit Box Scheme
 */
	/*!
 	 * \ingroup BoxModel
 	 * \defgroup BoxModels
         * \brief Models
 	 */
		/*!
		 * \ingroup BoxModels
		 * \defgroup ImmiscibleBoxModel
                 * \brief Immiscible Multi-Phase Model
                 *
                 * \copydetails Dumux::ImmiscibleModel
		 */
		/*!
		 * \ingroup BoxModels
		 * \defgroup PvsBoxModel
                 * \brief Compositional Model Using Primary Variable Switching
                 *
                 * \copydetails Dumux::PvsModel
		 */
		/*!
		 * \ingroup BoxModels
		 * \defgroup NcpBoxModel
                 * \brief Compositional Model Solving Non-linear Complementarity Problems
                 *
                 * \copydetails Dumux::NcpModel
		 */
		/*!
		 * \ingroup BoxModels
		 * \defgroup RichardsModel
                 * \brief Immiscible Model Using the Richards Equation
                 *
                 * \copydetails Dumux::RichardsModel
		 */
		/*!
		 * \ingroup BoxModels
		 * \defgroup BoxStokesModel
                 * \brief Compositional/Immiscible Model for the (Navier-)Stokes Equations
                 *
                 * \copydetails Dumux::StokesModel
		 */
	/*!
 	 * \ingroup BoxModel
 	 * \defgroup BoxModules
         * \brief Modules
 	 */
		/*!
		 * \ingroup BoxModules
		 * \defgroup BoxDarcyVelocity
                 * \brief Darcy Velocity Module 
                 *
                 * \copydetails Dumux::BoxDarcyVelocityModule
		 */

		/*!
		 * \ingroup BoxModules
		 * \defgroup BoxForchheimerVelocity
                 * \brief Forchheimer Velocity Module 
                 *
                 * \copydetails Dumux::BoxForchheimerVelocityModule
		 */

		/*!
		 * \ingroup BoxModules
		 * \defgroup BoxEnergy
                 * \brief Energy Equation
                 *
                 * \copydetails Dumux::BoxMultiPhaseEnergyModule
		 */

		/*!
		 * \ingroup BoxModules
		 * \defgroup BoxVtk
                 * \brief VTK Output Modules
		 */
	/*!
	 * \ingroup BoxModel
	 * \defgroup BoxTestProblems
         * \brief Test Problems
	 */

/****************** Semi-Implicit ******************/ 
/*!
 * \defgroup Sequential
 * \brief Semi-Implicit Models: IMPET (Implicit Pressure Explicit Transport)
 */
 
	/*!
	 * \ingroup Sequential
	 * \defgroup IMPET
         * \brief Models
	 */	 
		/*!
		 * \ingroup IMPET
		 * \defgroup OnePhase
                 * \brief Single Phase (FV)
		 */ 
		       /*!
			* \ingroup OnePhase
			* \defgroup Pressure1p
                        * \brief Pressure Equation
			*/
			       /*!
			 	* \ingroup Pressure1p
			 	* \defgroup FV1p
                                * \brief Finite Volumes
			 	*/	
		/*!
		 * \ingroup IMPET
		 * \defgroup IMPES
                 * \brief Immiscible IMPES
		 */		 
			/*!
			 * \ingroup IMPES
			 * \defgroup Pressure2p
                         * \brief Immiscible Pressure Equation
			 */
			 	/*!
			 	* \ingroup Pressure2p
			 	* \defgroup FVPressure2p
                                * \brief Finite Volume
			 	*/	
				/*!
				* \ingroup Pressure2p
			 	* \defgroup MimeticPressure2p
                                * \brief Mimetic Finite Differences
			 	*/	
			/*!
			 * \ingroup IMPES
			 * \defgroup Saturation2p
                         * \brief Immiscible Saturation Transport
			 */
			 	/*!
			 	* \ingroup Saturation2p
			 	* \defgroup FVSaturation2p
                                * \brief Finite Volume
			 	*/	
		/*!
		 * \ingroup IMPET
		 * \defgroup IMPEC
                 * \brief Miscible IMPEC
		 */
			/*!
			 * \ingroup IMPEC
			 * \defgroup multiphase
                         * \brief Compositional Multiphase Models
			 */
			/*!
			 * \ingroup IMPEC
			 * \defgroup multiphysics
                         * \brief Compositional Multiphysics Models
			 */
	/*!
	 * \ingroup Sequential
	 * \defgroup IMPETproblems
         * \brief Base Problems
	 */
	/*!
	 * \ingroup Sequential
	 * \defgroup IMPETProperties
         * \brief Properties
	 */
	/*!
	 * \ingroup Sequential
	 * \defgroup IMPETtests
         * \brief Test Problems
	 */

