         * \defgroup fluidmatrixinteractionslaws Laws
         * \brief Relations Describing Fluid-Matrix Interactions
         */
         /*!
         * \ingroup fluidmatrixinteractions
         * \defgroup fluidmatrixinteractionsparams Parameters
         * \brief Parameters for the Fluid-Matrix Interaction Laws
         */

/****************** Fully Implicit ******************/
/*!
 * \defgroup VcfvModel Vcfv Discretizations
 * \brief Fully Implicit Vertex Centered Finite Volume Scheme
 */
    /*!
     * \ingroup VcfvModel
     * \defgroup VcfvModels Models
     * \brief Models Using the Vcfv Scheme
     */
        /*!
         * \ingroup VcfvModels
         * \defgroup ImmiscibleVcfvModel Immiscible
         * \brief Immiscible Multi-Phase Model
         *
         * \copydetails Ewoms::ImmiscibleModel
         */
         /*!
          * \ingroup VcfvModels
          * \defgroup PvsVcfvModel PVS
          * \brief Compositional Multi-Phase Model Using Primary Variable Switching
          *
          * \copydetails Ewoms::PvsModel
          */
         /*!
          * \ingroup VcfvModels
          * \defgroup NcpModel NCP
          * \brief Compositional Multi-Phase Model Directly Solving Non-linear Complementarity Problems
          *
          * \copydetails Ewoms::NcpModel
          */
         /*!
          * \ingroup VcfvModels
          * \defgroup FlashModel Flash
          * \brief Compositional Multi-Phase Model Based on Flash Calculations
          *
          * \copydetails Ewoms::FlashModel
          */
         /*!
          * \ingroup VcfvModels
          * \defgroup BlackOilVcfvModel Black Oil
          * \brief Partially Miscible Three-Phase Model Based on the Black Oil Assumptions
          *
          * \copydetails Ewoms::BlackOilModel
          */
         /*!
          * \ingroup VcfvModels
          * \defgroup RichardsModel Richards
          * \brief Immiscible Two-Phase Model Using the Richards Equation
          *
          * \copydetails Ewoms::RichardsModel
          */
         /*!
          * \ingroup VcfvModels
          * \defgroup VcfvStokesModel (Navier-)Stokes
          * \brief Compositional/Immiscible Single-Phase Model for Turbulent Flow
          *
          * \copydetails Ewoms::StokesModel
          */
      /*!
       * \ingroup VcfvModel
       * \defgroup VcfvModules Modules
       * \brief Modules Used by the Vcfv Models
       */
          /*!
           * \ingroup VcfvModules
           * \defgroup VcfvDarcyVelocity Darcy Velocity
           * \brief The Darcy Velocity Module
           *
           * \copydetails Ewoms::VcfvDarcyVelocityModule
           */

          /*!
           * \ingroup VcfvModules
           * \defgroup VcfvForchheimerVelocity Forchheimer Velocity
           * \brief The Forchheimer Velocity Module
           *
           * \copydetails Ewoms::VcfvForchheimerVelocityModule
           */

          /*!
           * \ingroup VcfvModules
           * \defgroup VcfvDiffusion Molecular Diffusion
           * \brief A Module for to include molecular diffusion
           *
           * \copydetails Ewoms::VcfvDiffusionModule
           */

          /*!
           * \ingroup VcfvModules
           * \defgroup VcfvEnergy Energy
           * \brief A Module for the Energy Equation 
           *
           * \copydetails Ewoms::VcfvEnergyModule
           */

          /*!
           * \ingroup VcfvModules
           * \defgroup VcfvVtk VTK
           * \brief Various Modules to Write VTK Output
           */
    /*!
     * \ingroup VcfvModel
     * \defgroup VcfvTestProblems Test Problems
     * \brief Test Problems for the Vcfv Models
     */

/****************** Semi-Implicit ******************/
/*!
 * \defgroup Sequential Semi-Implicit Models
 * \brief IMPET (Implicit Pressure Explicit Transport)
 */

    /*!
     * \ingroup Sequential
     * \defgroup IMPET Models
     * \brief Semi-Implicit Models
     */
         /*!
          * \ingroup IMPET
          * \defgroup OnePhase Single Phase
          * \brief Single Phase (finite volumes)
          */
              /*!
               * \ingroup OnePhase
               * \defgroup Pressure1p Pressure Equation
               * \brief Pressure Equation
               */
                   /*!
                    * \ingroup Pressure1p
                    * \defgroup FV1p Finite Volumes
                    * \brief Finite Volumes Discretization
                    */
        /*!
         * \ingroup IMPET
         * \defgroup IMPES IMPES
         * \brief Immiscible IMPET
         */
             /*!
              * \ingroup IMPES
              * \defgroup Pressure2p Pressure Equation
              * \brief Pressure Equation for IMPES
              */
                  /*!
                   * \ingroup Pressure2p
                   * \defgroup FVPressure2p Finite Volumes
                   * \brief Finite Volumes
                   */
                  /*!
                   * \ingroup Pressure2p
                   * \defgroup MimeticPressure2p Mimetic Finite Differences
                   * \brief Mimetic Finite Differences
                   */
             /*!
              * \ingroup IMPES
              * \defgroup Saturation2p Saturation Transport
              * \brief Saturation Transport
              */
                  /*!
                   * \ingroup Saturation2p
                   * \defgroup FVSaturation2p Finite Volumes
                   * \brief Finite Volumes
                   */
        /*!
         * \ingroup IMPET
         * \defgroup IMPEC IMPEC
         * \brief Miscible IMPET
         */
            /*!
             * \ingroup IMPEC
             * \defgroup multiphase Multiphase Models
             * \brief Compositional Multiphase Models
             */
                /*!
                 * \ingroup multiphase
                 * \defgroup Adaptive2p2c h-Adaptive 2p2c Models
                 * \brief (Grid-)Adaptive Compositional Multiphase Models
                 */
            /*!
             * \ingroup IMPEC
             * \defgroup multiphysics Multiphysics Models
             * \brief Compositional Multiphysics Models
             */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETproblems Base Problems
     * \brief Base Problems
     */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETProperties Properties
     * \brief Properties
     */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETtests Test Problems
     * \brief Test Problems
     */

