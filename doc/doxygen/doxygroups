/****************** Fully Implicit ******************/
/*!
 * \defgroup BoxModel Box Scheme
 * \brief Fully Implicit Vertex Centered Finite Volune Scheme
 */
    /*!
     * \ingroup BoxModel
     * \defgroup BoxModels Models
     * \brief Models Using the Box Scheme
     */
        /*!
         * \ingroup BoxModels
         * \defgroup ImmiscibleBoxModel Immiscible
         * \brief Immiscible Multi-Phase Model
         *
         * \copydetails Dumux::ImmiscibleModel
         */
         /*!
          * \ingroup BoxModels
          * \defgroup PvsBoxModel PVS
          * \brief Compositional Multi-Phase Model Using Primary Variable Switching
          *
          * \copydetails Dumux::PvsModel
          */
         /*!
          * \ingroup BoxModels
          * \defgroup NcpBoxModel NCP
          * \brief Compositional Multi-Phase Model Directly Solving Non-linear Complementarity Problems
          *
          * \copydetails Dumux::NcpModel
          */
         /*!
          * \ingroup BoxModels
          * \defgroup FlashModel Flash
          * \brief Compositional Multi-Phase Model Based on Flash Calculations
          *
          * \copydetails Dumux::FlashModel
          */
         /*!
          * \ingroup BoxModels
          * \defgroup BlackOilBoxModel Black Oil
          * \brief Partially Miscible Three-Phase Model Based on the Black Oil Assumptions
          *
          * \copydetails Dumux::BlackOilModel
          */
         /*!
          * \ingroup BoxModels
          * \defgroup RichardsModel Richards
          * \brief Immiscible Two-Phase Model Using the Richards Equation
          *
          * \copydetails Dumux::RichardsModel
          */
         /*!
          * \ingroup BoxModels
          * \defgroup BoxStokesModel (Navier-)Stokes
          * \brief Compositional/Immiscible Single-Phase Model for Turbolent Flow
          *
          * \copydetails Dumux::StokesModel
          */
      /*!
       * \ingroup BoxModel
       * \defgroup BoxModules Modules
       * \brief Modules
       */
          /*!
           * \ingroup BoxModules
           * \defgroup BoxDarcyVelocity Darcy Velocity
           * \brief Darcy Velocity Module
           *
           * \copydetails Dumux::BoxDarcyVelocityModule
           */

          /*!
           * \ingroup BoxModules
           * \defgroup BoxForchheimerVelocity Forchheimer Velocity
           * \brief Forchheimer Velocity Module
           *
           * \copydetails Dumux::BoxForchheimerVelocityModule
           */

          /*!
           * \ingroup BoxModules
           * \defgroup BoxEnergy Energy
           * \brief Provides the Energy Equation
           *
           * \copydetails DoxyEnergyModule
           */

          /*!
           * \ingroup BoxModules
           * \defgroup BoxVtk VTK
           * \brief Various VTK Output Modules
           */
    /*!
     * \ingroup BoxModel
     * \defgroup BoxTestProblems Test Problems
     * \brief Test Problems for the Box Models
     */

/****************** Semi-Implicit ******************/
/*!
 * \defgroup Sequential Semi-Implicit Models
 * \brief IMPET (Implicit Pressure Explicit Transport)
 */

    /*!
     * \ingroup Sequential
     * \defgroup IMPET Models
     * \brief Semi-Implicit Models
     */
         /*!
          * \ingroup IMPET
          * \defgroup OnePhase Single Phase
          * \brief Single Phase (finite volumes)
          */
              /*!
               * \ingroup OnePhase
               * \defgroup Pressure1p Pressure Equation
               * \brief Pressure Equation
               */
                   /*!
                    * \ingroup Pressure1p
                    * \defgroup FV1p Finite Volumes
                    * \brief Finite Volumes Discretization
                    */
        /*!
         * \ingroup IMPET
         * \defgroup IMPES IMPES
         * \brief Immiscible IMPET
         */
             /*!
              * \ingroup IMPES
              * \defgroup Pressure2p Pressure Equation
              * \brief Pressure Equation for IMPES
              */
                  /*!
                   * \ingroup Pressure2p
                   * \defgroup FVPressure2p Finite Volumes
                   * \brief Finite Volumes
                   */
                  /*!
                   * \ingroup Pressure2p
                   * \defgroup MimeticPressure2p Mimetic Finite Differences
                   * \brief Mimetic Finite Differences
                   */
             /*!
              * \ingroup IMPES
              * \defgroup Saturation2p Saturation Transport
              * \brief Saturation Transport
              */
                  /*!
                   * \ingroup Saturation2p
                   * \defgroup FVSaturation2p Finite Volumes
                   * \brief Finite Volumes
                   */
        /*!
         * \ingroup IMPET
         * \defgroup IMPEC IMPEC
         * \brief Miscible IMPET
         */
            /*!
             * \ingroup IMPEC
             * \defgroup multiphase Multiphase Models
             * \brief Compositional Multiphase Models
             */
            /*!
             * \ingroup IMPEC
             * \defgroup multiphysics Multiphysics Models
             * \brief Compositional Multiphysics Models
             */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETproblems Base Problems
     * \brief Base Problems
     */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETProperties Properties
     * \brief Properties
     */
    /*!
     * \ingroup Sequential
     * \defgroup IMPETtests Test Problems
     * \brief Test Problems
     */

